from tkinter import *

window = Tk()

canvas = Canvas(window, height=500, width=500)
# canvas.create_rectangle(50, 50, 250, 250, fill='green')
canvas.create_arc(10, 20, 490, 500, extent=180, fill='#c20215', width=6)
canvas.create_arc(10, 20, 490, 500, extent=180,start=180, fill='white', width=6)
canvas.create_oval(190, 190, 320, 320, fill='White', width=4)
canvas.pack()

window.mainloop()
