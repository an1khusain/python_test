from tkinter import *

window = Tk()

frame = Frame(window, bg='#1cb84d', bd=5, relief=RAISED)
frame.place(x=0, y=0)

Button(frame, text='A', font=('Comid Sans', 25), width=3).pack(side=TOP)
Button(frame, text='N', font=('Comid Sans', 25), width=3).pack(side=LEFT)
Button(frame, text='I', font=('Comid Sans', 25), width=3).pack(side=LEFT)
Button(frame, text='K', font=('Comid Sans', 25), width=3).pack(side=LEFT)

window.mainloop()
